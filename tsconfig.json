{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["app/*"],
      "@constants/*": ["app/constants/*"],
      "@components/*": ["app/components/*"],
      "@navigation/*": ["app/navigation/*"],
      "@hooks/*": ["app/hooks/*"],
      "@screens/*": ["app/screens/*"],
      "@schemas/*": ["app/schemas/*"],
      "@db/*": ["app/db/*"],
      "@utils/*": ["app/utils/*"],
    }
  }
}
